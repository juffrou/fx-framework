<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="401.0" prefWidth="581.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="org.juffrou.fx.business.ctrl.PersonController">
   <children>
      <VBox layoutX="32.0" layoutY="31.0" prefHeight="200.0" prefWidth="259.0">
         <children>
            <Label text="Name" />
            <TextField fx:id="name">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </TextField>
            <Label text="Email" />
            <TextField fx:id="email">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </TextField>
            <Label text="Birthday" />
            <DatePicker fx:id="dateOfBirth" />
         </children>
      </VBox>
      <Button layoutX="499.0" layoutY="361.0" mnemonicParsing="false" onAction="#save" text="Save" />
      <Button layoutX="425.0" layoutY="361.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
